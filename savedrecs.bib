@article{ ISI:000438480000013,
Author = {Querner, Pascal and Sterflinger, Katja and Piombino-Mascali, Dario and
   Morrow, Johnica J. and Pospischil, Reiner and Pinar, Guadalupe},
Title = {{Insect pests and Integrated Pest Management in the Capuchin Catacombs of
   Palermo, Italy}},
Journal = {{INTERNATIONAL BIODETERIORATION \& BIODEGRADATION}},
Year = {{2018}},
Volume = {{131}},
Number = {{SI}},
Pages = {{107-114}},
