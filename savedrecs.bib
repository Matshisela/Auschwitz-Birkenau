@article{ ISI:000438480000006,
Author = {Komorowicz, Magdalena and Wroblewska, Hanna and Fojutowski, Andrzej and
   Kropacz, Aleksandra and Noskowiak, Andrzej and Pomian, Iwona},
Title = {{The impact of 5 years' underwater exposure in the Baltic Sea (Puck Bay)
   on selected properties of English oak wood samples}},
Journal = {{INTERNATIONAL BIODETERIORATION \& BIODEGRADATION}},
Year = {{2018}},
Volume = {{131}},
Number = {{SI}},
Pages = {{40-50}},
Month = {{JUL}},
