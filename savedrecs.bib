@article{ ISI:000438480000011,
Author = {Matusiak, Katarzyna and Machnowski, Waldemar and Wrzosek, Henryk and
   Polak, Jowita and Rajkowska, Katarzyna and Smigielski, Krzysztof and
   Kunicka-Styczynska, Alina and Gutarowska, Beata},
Title = {{Application of Cinnamomum zeylanicum essential oil in vapour phase for
   heritage textiles disinfection}},
Journal = {{INTERNATIONAL BIODETERIORATION \& BIODEGRADATION}},
Year = {{2018}},
Volume = {{131}},
Number = {{SI}},
Pages = {{88-96}},
Month = {{JUL}},
