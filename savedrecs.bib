@article{ ISI:000438480000002,
Author = {Dyda, Magdalena and Decewicz, Przemyslaw and Romaniuk, Krzysztof and
   Wojcieszak, Martyna and Sklodowska, Aleksandra and Dziewit, Lukasz and
   Drewniak, Lukasz and Laudy, Agnieszka},
Title = {{Application of metagenomic methods for selection of an optimal growth
   medium for bacterial diversity analysis of microbiocenoses on historical
   stone surfaces}},
Journal = {{INTERNATIONAL BIODETERIORATION \& BIODEGRADATION}},
Year = {{2018}},
Volume = {{131}},
Number = {{SI}},
