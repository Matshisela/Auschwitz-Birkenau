@article{ ISI:000438480000010,
Author = {Brzozowska, Iwona and Bogdanowicz, Albert and Szczesny, Pawei and
   Zielenkiewicz, Urszula and Laudy, Agnieszka},
Title = {{Evaluation of bacterial diversity on historical silk velvet textiles
   from the Museum of King John III's Palace at Wilanow, Poland}},
Journal = {{INTERNATIONAL BIODETERIORATION \& BIODEGRADATION}},
Year = {{2018}},
Volume = {{131}},
Number = {{SI}},
