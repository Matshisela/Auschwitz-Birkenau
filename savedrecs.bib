@article{ ISI:000558905000009,
Author = {Halioua, Bruno and Marmor, Michael F.},
Title = {{The eyes of the angel of death: Ophthalmic experiments of Josef Mengele}},
Journal = {{SURVEY OF OPHTHALMOLOGY}},
