@article{ ISI:000355959800002,
Author = {Seeger, Adina},
Title = {{Architect of Auschwitz-Birkenau, Defendant in the Vienna Auschwitz
   Trial: Fritz Ertl - Career of a Nazi Perpetrator}},
Journal = {{ZEITGESCHICHTE}},
Year = {{2015}},
Volume = {{42}},
Number = {{2}},
Pages = {{84+}},
Month = {{MAR-APR}},
Abstract = {{While Auschwitz-Birkenau has been a focal point in historic research
   after 1945 as well as in the sociopolitical context, the knowledge about
   its planning and development is scant. The history of construction of
   the concentration camp still is insufficiently researched. Thus, this
   paper attempts to delineate the career of Fritz Ertl, who was an
   architect in Auschwitz from 1940 to 1943 where he contributed decisively
   to the planning and construction of the concentration camp of Birkenau.
   The analysis of Ertl's career is based on considerations and tendencies
   in the current research on Nazi perpetrators, and, furthermore, examines
   the aftermath of Ertl's SS-tenure: After 1945 Ertl managed to fight his
   classification as ``Belasteter{''}, and was acquitted of all charges in
   the Vienna Auschwitz Trial in 1972.}},
Publisher = {{VANDENHOECK \& RUPRECHT GMBH \& CO KG}},
